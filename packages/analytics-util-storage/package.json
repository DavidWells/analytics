  "files": [
    "lib",
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidWells/analytics.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5"
  },
  "dependencies": {
    "@analytics/cookie-utils": "^0.2.5",
    "@analytics/global-storage-utils": "^0.1.1",
    "@analytics/localstorage-utils": "^0.1.3"
  }
