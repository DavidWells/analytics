    "release:major": "npm version major && npm publish",
    "es": "../../node_modules/.bin/babel-node ./testBabel.js"
  },
  "files": [
    "lib",
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidWells/analytics.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5"
